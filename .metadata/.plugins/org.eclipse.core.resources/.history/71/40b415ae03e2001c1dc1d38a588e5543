package bytebank;

public class Conta {
		double saldo;
		int agencia;
		int numero;
		String titular;
	
	public Conta(double saldo,int agencia, int numero,String titular) {
		this.saldo = saldo;
		this.agencia = agencia;
		this.numero = numero;
		this.titular = titular;
	}
	@Override
	public String toString() {
		return "Saldo: " + this.saldo + "Titular: " + this.titular;
	}

}
